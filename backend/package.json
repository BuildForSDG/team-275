{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "node dist/app.js",
    "start": "nodemon --exec babel-node src/app.js",
    "build": "babel src/ --out-dir dist",
    "format": "prettier --write \"src/**/*.js\" \"lib/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\" \"lib/**/*.js\"",
    "clean": "rimraf dist",
    "lint": "npm run clean && eslint . src/",
    "test": "echo No test specified"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.12",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2"
  }
}
